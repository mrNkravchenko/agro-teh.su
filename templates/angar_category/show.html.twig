{% extends 'base_angar.html.twig' %}

{% block title %}AngarCategory{% endblock %}

{% block content %}
    <h1>AngarCategory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ angar_category.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ angar_category.name }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ angar_category.url }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ angar_category.image }}</td>
            </tr>
            <tr>
                <th>Small_image</th>
                <td>{{ angar_category.smallImage }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ angar_category.created ? angar_category.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ angar_category.updated ? angar_category.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('angar_category_index') }}">back to list</a>

    <a href="{{ path('angar_category_edit', {'id': angar_category.getId | number_format }) }}">edit</a>

    {{ include('angar_category/_delete_form.html.twig') }}
{% endblock %}
