{% extends 'base.html.twig' %}

{% block title %}TechCategory{% endblock %}

{% block body %}
    <h1>TechCategory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tech_category.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ tech_category.name }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ tech_category.title }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ tech_category.url }}</td>
            </tr>
            <tr>
                <th>Small_image</th>
                <td>{{ tech_category.smallImage }}</td>
            </tr>
            <tr>
                <th>Side_bar_image</th>
                <td>{{ tech_category.sideBarImage }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ tech_category.content }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ tech_category.created ? tech_category.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ tech_category.updated ? tech_category.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('tech_category_index') }}">back to list</a>

    <a href="{{ path('tech_category_edit', {'id': tech_category.id}) }}">edit</a>

    {{ include('tech_category/_delete_form.html.twig') }}
{% endblock %}
